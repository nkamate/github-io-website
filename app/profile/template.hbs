<div class="row">
    <div class="col l5 s12 m12">
        <div class="card blue-grey darken-1 hoverable">
            <div class="card-content white-text">
                <div class="card-title"><h4>About Me</h4></div>
              {{#each model as |profile|}}
                {{#each profile.summary as |summary|}}
                    <blockquote> {{summary}}</blockquote>
                {{/each}}
              {{/each}}
            </div>
        </div>
        <div class="card teal darken-1">
            <div class="card-content white-text">
                <div class="card-title"><h4>Expertise</h4></div>
                <div class="row">
                    <div class="col s2"><i class="fa fa-git fa-2x"></i></div>
                    <div class="col s2"><i class="fa fa-linux fa-2x"></i></div>
                    <div class="col s2"><i class="fa fa-windows fa-2x"></i></div>
                    <div class="col s2"><i class="fa fa-git fa-2x"></i></div>
                    <div class="col s2"><i class="fa fa-git fa-2x"></i></div>
                    <div class="col s2"><i class="fa fa-git fa-2x"></i></div>
                </div>
            </div>
        </div>
    </div>

    <div class="col l7 s12 m12 timeline-column">
        <h4 class="center black-text">Timeline</h4>

        <div class="timeline_holder">
            <ul class="cbp_tmtimeline">
              {{#each model as |profile|}}
                {{#each profile.events as |event|}}
                    <li>
                        <time class="cbp_tmtime">
                            <span>{{event.year}}</span>
                        </time>
                        <div class="cbp_tmicon cbp_tmicon-phone"></div>
                        <div class="cbp_tmlabel">
                            <h5>{{event.title}}</h5>
                            <ul>
                              {{#each event.info as |info|}}
                                  <li style="list-style: square;">{{info}}</li>
                              {{/each}}
                            </ul>
                        </div>
                    </li>
                {{/each}}
              {{/each}}
            </ul>
        </div>
    </div>
</div>